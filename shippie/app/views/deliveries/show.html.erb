<h2><%= @delivery.name %></h2>
<%= link_to "Edit Delivery", edit_delivery_path(@delivery) %>
<%= link_to "Delete Delivery", @delivery, :method => :delete,
  :confirm => "Are you sure you want to delete this delivery?" %>
<%= link_to "New Bid", new_delivery_bid_path(@delivery) %>
<p>Bids<br>
<ul id='bids'>
  <% @delivery.bids.each do |bid| %>
    <li>
    <%= bid.price %>
    </li>
  <% end %>
</ul>
</p>
